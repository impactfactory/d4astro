---
import Arrival from "../../components/Sections/Arrival.astro";
import Faq from "../../components/Sections/Faq.astro";
import Front from "../../layouts/Front.astro";
import Intro from "../../components/Sections/Intro.astro";
import Practice from "../../components/Sections/Practice.astro";
import Reservation from "../../components/Sections/Reservation.astro";
import Team from "../../components/Sections/Team.astro";

import { Debug } from "astro/components";

import data from "../../data/de.json";
---

<Front title={data.meta.title} />
<Debug {data} />
{data.test}
<main>
  <Intro slogan={data.intro.title} text={data.intro.text} />
  <Team />
  <Practice title={data.practice.title} text={data.practice.text} />
  <Reservation title={data.reservation.title} text={data.reservation.text} />
  <Arrival title={data.arrival.title} text={data.arrival.text} />
  <Faq />
</main>
